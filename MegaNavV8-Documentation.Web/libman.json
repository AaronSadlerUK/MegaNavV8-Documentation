{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@4.6.0",
      "destination": "lib/bootstrap/",
      "files": [
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery.scrollto@2.1.3",
      "destination": "lib/jquery.scrollto/",
      "files": [
        "jquery.scrollTo.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "ekko-lightbox@5.3.0",
      "destination": "lib/ekko-lightbox/",
      "files": [
        "ekko-lightbox.min.js"
      ]
    }
  ]
}